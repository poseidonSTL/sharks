            <string>if (state == "normal")
{
    // Move normally


    // If you reach the diver's row, switch to the hunt state.
    if(y &lt;= obj_diver.y)
    {
        state = "hunt";
        huntTimeTotal = GetSharkHuntTime();
        
        startX = x;
        startY = y;
    }
}
else if (state == "hunt")
{   
    var currentDistance = point_distance(x,y,obj_diver.x, obj_diver.y);
    distance = point_distance(startX,startY,obj_diver.x,obj_diver.y);
    distanceToMove = distance / huntTimeTotal;
    
    huntTime += 1;
    if(huntTime &gt; huntTimeTotal)
    {
        huntTime = huntTimeTotal;
    }

    if (currentDistance &gt; distanceToMove)
    {
        move_towards_point(obj_diver.x, obj_diver.y, distanceToMove);
        image_angle = point_direction(x,y,obj_diver.x,obj_diver.y) - 90;
    }
    else
    {
        x = obj_diver.x;
        y = obj_diver.y;
    }
}
else if (state == "spinout")
{
    increment = 360 / (global.sharkSpinoutTime * global.gameSpeed);
    image_angle += increment;
}

if(waveTime &gt; 0)
{
    timeFactor = waveTime / global.sharkTimeToTopOfWave;
    scaleFactor = global.sharkScaleAtTopOfWave - 1.0;
    tween = sin(timeFactor * pi/2);
    scale = 1 + (scaleFactor * tween);
    image_xscale = scale;
    image_yscale = scale;

    waveTime += 1;
    if(waveTime &gt; global.sharkTimeToTopOfWave)
    {
        waveTime = global.sharkTimeToTopOfWave;
    }
}
</string>
