        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Lane Switching

// Keyboard input
if (laneSwitchTime &gt; 0)
{
}
else if (keyboard_check_pressed(vk_left) || keyboard_check_pressed(ord('A')))
{
    if(prevLane &gt; 0)
    {
        targetLane = prevLane - 1;
        audio_play_sound(swimmerMove, 10, false);
    }
    
    prevLaneCommand = "left";
    laneSwitchTime = 1;
}
else if (keyboard_check_pressed(vk_right) || keyboard_check_pressed(ord('D')))
{
    if(prevLane &lt; 2)
    {
        targetLane = prevLane + 1;
        audio_play_sound(swimmerMove, 10, false);
    }
    
    prevLaneCommand = "right";
    laneSwitchTime = 1;
}

// Movement
if (laneSwitchTime &gt; 0)
{
    // Position
    var distanceRemaining = global.laneX[targetLane] - x;
    var dir = sign(distanceRemaining);
    
    var tween = 0;
    var timeFactor = laneSwitchTime / global.laneSwitchTime;
    if(laneSwitchTime &lt;= global.laneSwitchTime/2)
    {
        tween = power((timeFactor*2),2)/2;
    }
    else
    {
        tween = 0.5 + (sqrt((timeFactor - 0.5)*2)/2);
    }
    x = global.laneX[prevLane] + (tween * dir * global.distanceBetweenLanes);
    
    // Tilt
    maxTilt = global.laneSwitchMaxTilt;
    if(prevLaneCommand = "right")
    {
        maxTilt = -maxTilt;
    }
    tween = sin(timeFactor * pi);
    image_angle = tween * maxTilt;
    
    // time tracking
    laneSwitchTime += 1;
    if(laneSwitchTime &gt; global.laneSwitchTime)
    {
        laneSwitchTime = 0;
        prevLane = targetLane;
        x = global.laneX[targetLane];
    }
}

</string>
          </argument>
        </arguments>
